{
  "version": 2,
  "name": "ionic4-vue-app-sidemenu",
  "alias": "ionic4-vue-app-sidemenu",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    { "src": "^/js/(.*)", "dest": "/js/$1" },
    { "src": "^/css/(.*)", "dest": "/css/$1" },
    { "src": "^/img/(.*)", "dest": "/img/$1" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": ".*", "headers": {"cache-control": "max-age=0,must-revalidate"}, "dest": "/index.html" }
  ]
}